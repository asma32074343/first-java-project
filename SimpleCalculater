package calculater;

import java.math.*;
import java.security.*;
import java.text.*;
import java.util.*;
import java.util.concurrent.*;
/*
*
*/




public class cal {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		float y = in.nextFloat();
     
		float x=in.nextFloat();
     
		float resultA=x+y;
		float resultB=x-y;
		float resultC=x/y;
		float resultD=x*y;
   	System.out.println("this is the result of the Addition");   
	System.out.println(resultA);
	System.out.println("this is the result of the Subtraction");   
	System.out.println(resultB);
	System.out.println("this is the result of the Division");   
	System.out.println(resultC);
	System.out.println("this is the result of the Multiplication");   
	System.out.println(resultD);
	}
}

# first-java-project
java project
package dadi;

public class Main {

    public static void main (String[] args) {
        int n = 10;
        int n1 = 0;
        int n2 = 1;
        int n3;
        for (int i=0;i<n;i++) {
            n3 = n1 + n2;
            n1 = n2;
            n2 = n3;
            System.out.print(" "+n3);
        }
    }
}
